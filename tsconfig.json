{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "module": "ESNext",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "target": "es2017"
    },
    "formatCodeOptions": {
        "indentSize": 4,
        "tabSize": 4
    },
    "exclude": ["node_modules"]
}
